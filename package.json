{
    "name": "poe-api",
    "version": "1.0.6",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "description": "A minimal module exposing the Path of Exile stash tabs api.",
    "files": [
        "dist/**/*"
    ],
    "author": "Stephen Poole",
    "scripts": {
        "start": "npm run prod",
        "prod": "webpack --mode=production && npm run docs",
        "dev": "webpack -w --mode=development",
        "example": "cross-env HOSTNAME=api.pathofexile.com ts-node example/index.ts",
        "docs": "typedoc --excludeExternals --externalPattern \"**/node_modules/**\" --ignoreCompilerErrors --name \"Path of Exile\" --mode \"file\" --out \"docs/poe\" \"src\"",
        "test": "nyc mocha --file ./test/setup.ts -r ts-node/register test/**/*.spec.ts",
        "coverage": "nyc --reporter=lcov npm test",
        "snyk-protect": "snyk protect",
        "prepare": "yarn run snyk-protect"
    },
    "license": "MIT",
    "dependencies": {
        "axios": "^0.19.2",
        "snyk": "^1.316.1"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@types/chai": "^4.2.11",
        "@types/mocha": "^7.0.2",
        "@types/moxios": "^0.4.9",
        "@types/node": "13.11.1",
        "@types/webpack": "^4.4.24",
        "@types/webpack-env": "^1.13.7",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "awesome-typescript-loader": "^5.2.1",
        "chai": "^4.2.0",
        "circular-dependency-plugin": "^5.2.0",
        "codecov": "^3.6.5",
        "cross-env": "^7.0.2",
        "dotenv-webpack": "^1.7.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-loader": "^4.0.0",
        "eslint-plugin-import": "^2.16.0",
        "mocha": "^7.1.1",
        "moxios": "^0.4.0",
        "nyc": "^15.0.1",
        "source-map": "^0.7.3",
        "source-map-loader": "^0.2.4",
        "source-map-support": "^0.5.10",
        "ts-node": "^8.9.0",
        "typedoc": "^0.17.4",
        "typescript": "^3.8.3",
        "webpack": "^4.29.5",
        "webpack-cli": "^3.2.3",
        "webpack-node-externals": "^1.7.2"
    },
    "snyk": true
}
